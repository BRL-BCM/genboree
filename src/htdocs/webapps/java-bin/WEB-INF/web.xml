<?xml version="1.0" encoding="ISO-8859-1"?>

<!DOCTYPE web-app
    PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
    "http://java.sun.com/dtd/web-app_2_3.dtd">

<web-app>

    <servlet>
        <servlet-name>ProgressServlet</servlet-name>
        <servlet-class>org.genboree.upload.ProgressServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>FileRetriever</servlet-name>
        <servlet-class>org.genboree.util.FileRetriever</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>VGPDownloader</servlet-name>
        <servlet-class>org.genboree.svg.VGPDownloader</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>ProgressServlet</servlet-name>
        <url-pattern>/servlets/ProgressServlet</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>FileRetriever</servlet-name>
        <url-pattern>/servlets/FileRetriever</url-pattern>
    </servlet-mapping>


    <servlet-mapping>
        <servlet-name>VGPDownloader</servlet-name>
        <url-pattern>/servlets/VGPDownloader</url-pattern>
    </servlet-mapping>


</web-app>
