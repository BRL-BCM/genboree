  <span class="punct">&quot;</span><span class="string">displayName</span><span class="punct">&quot;</span> <span class="punct">:</span> <span class="punct">&quot;&lt;</span><span class="regex">value</span><span class="punct">&gt;&quot;,</span>       <span class="comment"># The human readable (pretty) name for the resource -- optional.</span>
  <span class="punct">&quot;</span><span class="string">templateURI</span><span class="punct">&quot;</span> <span class="punct">:</span> <span class="punct">&quot;&lt;</span><span class="regex">value</span><span class="punct">&gt;&quot;,</span>       <span class="comment"># The template URI for the resource described by this object.</span>
