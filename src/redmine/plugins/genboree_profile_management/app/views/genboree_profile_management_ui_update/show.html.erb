
<%
  @loginDisabled = true 
%>
<div class="row">
  <div class="col-lg-3">
  </div>
  <div class="col-lg-6 col-sm-12 col-xs-12">
    <%= form_tag ({ :controller => :genboree_profile_management_ui_update, :action => :update } , "method" => :post, "data-toggle" => "validator") do %>
      <h2>Update Profile</h2>
      <div class="well">
       <div class="row form-row">
        <%= render :partial => "genboree_profile_management_ui_entry/login_id" %>
        <%= render :partial => "genboree_profile_management_ui_entry/basic_user_info" %>
        <div class="col-sm-12 submit-button">
         <%= submit_tag "Save changes", "class" => "btn btn-primary" %>
        </div>
       </div>
      </div>
    <% end %>
  </div>
</div>
