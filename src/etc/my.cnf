[mysqld]
socket                        = /usr/local/brl/local/var/mysql.sock
port                          = 16002
pid-file                      = /usr/local/brl/local/var/mysql.pid
log_error                     = /usr/local/brl/local/var/mysql_error.log
tmpdir                        = /usr/local/brl/local/tmp/mysql
basedir                       = /usr/local/brl/local/mysql
datadir                       = /usr/local/brl/data/mysql/data
user                          = genboree
key_buffer_size               = 268435456
#table_cache                   = 192
read_buffer_size              = 8388608
read_rnd_buffer_size          = 4194304
sort_buffer_size              = 16777216
myisam_sort_buffer_size       = 33554432
join_buffer_size              = 2097152
query_cache_type              = 1
query_cache_size              = 67108864
query_cache_limit             = 768000
max_allowed_packet            = 280M
thread_cache_size             = 4
query_prealloc_size           = 32768
thread_stack                  = 393216 
max_connections               = 4096
wait_timeout                  = 10800

